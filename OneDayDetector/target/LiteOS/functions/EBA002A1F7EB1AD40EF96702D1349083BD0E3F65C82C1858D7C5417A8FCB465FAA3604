int FindFunction_WhichIsBroken(int NumberToFind)
{
    int i = 0;
    while (i <= 8) /* Notice I should have been in braces */
        i++;
        if (NumbersToFind[i] == NumberToFind) /* Yikes!  I'm getting run after the loop finishes instead of during it! */
            return i;
    return 0;
}
