typedef struct __config {
    char *name;
    uint32_t usart_port;
    uint32_t buardrate;
    uint32_t linkid_num;
    uint32_t user_buf_len; /* malloc 3 block memory for intener use, len * 3 */
    char *cmd_begin;
    char *line_end;
    uint32_t  mux_mode;
    uint32_t timeout; /* command respond timeout */
} at_config;
