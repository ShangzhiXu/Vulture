struct MMUFirstLevelPageTable {
    UINT32 id : 2;            /* [1 : 0] */
    UINT32 pxn : 1;           /* [2] */
    UINT32 ns : 1;            /* [3] */
    UINT32 sbz : 1;           /* [4] */
    UINT32 domain : 4;        /* [8 : 5] */
    UINT32 imp : 1;           /* [9] */
    UINT32 baseAddress : 22;  /* [31 : 10] */
};
